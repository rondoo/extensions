<UserControl x:Class="Signum.Windows.Authorization.PasswordExpiresInterval"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Authorization;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="{x:Type d:PasswordExpiresIntervalEntity}" MinWidth="300">
    <StackPanel >
        <m:ValueLine  m:Common.Route="Enabled"/>
        <m:ValueLine  m:Common.Route="Days"/>
        <m:ValueLine  m:Common.Route="DaysWarning"/>
    </StackPanel>
</UserControl>
