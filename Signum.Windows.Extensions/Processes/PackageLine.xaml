<UserControl x:Class="Signum.Windows.Processes.PackageLine"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Processes;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="{x:Type d:PackageLineEntity}"
    MinWidth="300">
    <StackPanel>
        <m:EntityLine m:Common.Route="Package" />
        <m:EntityLine m:Common.Route="Target" />
        <m:EntityLine m:Common.Route="Result"  m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="FinishTime" />
    </StackPanel>
</UserControl>
