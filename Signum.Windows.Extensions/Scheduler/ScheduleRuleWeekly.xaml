<UserControl x:Class="Signum.Windows.Scheduler.ScheduleRuleWeekly"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Scheduler;assembly=Signum.Entities.Extensions"
    xmlns:sws="clr-namespace:Signum.Windows.Scheduler"
    m:Common.TypeContext="{x:Type d:ScheduleRuleWeeklyEntity}"
    Padding="2">
    <StackPanel>
        <m:ValueLine m:Common.Route="DayOfTheWeek" />
        <m:ValueLine m:Common.Route="StartingOn"/>
    </StackPanel>
</UserControl>
