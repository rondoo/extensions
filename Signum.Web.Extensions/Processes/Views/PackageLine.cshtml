@using Signum.Entities.Processes

@using (var e = Html.TypeContext<PackageLineEntity>())
{
    e.ReadOnly = true;
    
    @Html.EntityLine(e, f => f.Package)
    @Html.EntityLine(e, f => f.Target)
    @Html.EntityLine(e, f => f.Result)
    @Html.ValueLine(e, f => f.FinishTime)

    @Html.SearchControl(new FindOptions(typeof(ProcessExceptionLineEntity), "Line", e.Value), new Context(e, "Exceptions"))
} 