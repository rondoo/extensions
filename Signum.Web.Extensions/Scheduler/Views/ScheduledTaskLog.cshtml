@using Signum.Engine
@using Signum.Entities.Scheduler

@using (var e = Html.TypeContext<ScheduledTaskLogEntity>()) 
{
    @Html.EntityLine(e, f => f.Task)
    @Html.EntityLine(e, f => f.ScheduledTask)
	@Html.ValueLine(e, f => f.StartTime)
    @Html.ValueLine(e, f => f.EndTime)
    @Html.EntityLine(e, f => f.User)
    @Html.EntityLine(e, f => f.ProductEntity)
    @Html.ValueLine(e, f => f.MachineName)
    @Html.ValueLine(e, f => f.ApplicationName)
    @Html.EntityLine(e, f => f.Exception)
}